};
class test{
    public:
        string name;
        string date;
        int score;

        test(string N,string D,int S){
            name=N;
            date=D;
            score=S;
        }
    };

int main(){
    test abc("John","April 5th", 95);
    cout<<abc.name;
    return 0;
}
